	pages = {892},
	file = {PubMed Central Full Text PDF:/home/emilegp/Zotero/storage/5GDYYMD5/Seo et al. - 2017 - Mechanical Contact Characteristics of PC3 Human Prostate Cancer Cells on Complex-Shaped Silicon Micr.pdf:application/pdf},
}

